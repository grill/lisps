%!
(util.ps) run

/printTokens {
   isOpenPT
   { (\() print (\n) print 
     token pop mark exch
     {
         printTokens
	 token
	 not
	 { (\)) print (\n) print exit }
	 if
         getTokenName % token - string
	 exch
     } loop
   }
   if
} def

(Lisp Interpreter by Alex and Gabriel
)
print
{
   % print
   flush
   readLineStdin % - string bool
   not
   % (%stdin) (r)file token
   { pop exit }
   if
   printTokens
   pstack
} loop
