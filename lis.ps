%!
(util.ps) run
(eval.ps) run

/mytoken {
  isOpenPT
  { token mark exch }
  { token }
  ifelse

  { exch mytoken }
  { 
    count 2 le not  % initial token makes problems could also be solved by calling token once before calling mytoken
    { eval exch mytoken } % )
    if
  }
  ifelse
} def

(Lisp Interpreter by Alex and Gabriel
)
print
{
   % print
   flush
   readLineStdin % - string bool
   not
   % (%stdin) (r)file token
   { pop exit }
   if
   % isOpenPT
   % { token pop exch pop mark exch pstack (\n\n\nasdfasdf\n) print  mytoken }
   % if
   % printTokens
   % pop

   mytoken
   pstack
   clear
} loop
