%!
(util.ps) run
(eval.ps) run

/parse {
  isOpenPT
  { token mark exch }
  { token }
  ifelse

  { exch parse }
  { 
    count 2 gt  % initial token makes problems could also be solved by calling token once before calling mytoken
    { eval exch parse } % )
    if
  }
  ifelse
} def

/lisps
{
   (Lisp Interpreter by Alex and Gabriel\n)
   print
   {
   (lis.ps >) print flush
      readLineStdin % - string bool
      not
      { pop exit }
      if
      
      parse
      pstack
      clear
   } loop
   clearvars
} def
